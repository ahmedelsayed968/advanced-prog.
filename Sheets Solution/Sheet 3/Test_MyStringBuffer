public class Main {


    /**
     * @param args the command line arguments
     */
    public static void main(String args[]) {
        MyStringBuffer test = new MyStringBuffer("Hello World");
        test.print();
        System.out.println(test.indexOf('x'));
        System.out.println(test.length());
        test.erase(2, 3);
        test.print();
        test.insert(2, "llo");
        test.print();
    }
}
